# vim: set ft=fish :
set __plg_dir__ (dirname (status -f))
set __alts__    "$__plg_dir__/alternatives.fish"
if not functions -q require
    set_color red
    echo "The `require` plugin must be installed in order to use alternatives" 1>&2
    set_color normal
else if [ -f $__alts__ ]
    source $__alts__
else
    set_color red
    echo "$__alts__ not found" 1>&2
    set_color normal
end
